source $VIMRUNTIME/gvimrc_example.vim

" set transparency=10
set background=dark
" highlight Normal guifg=gray guibg=black

" Hide toolbar for more screen estate
set guioptions-=T
" But show the tab bar when we have more than one tab
set guioptions+=e

" Set font
set guifont=Fira\ Mono\ Regular\ 10
