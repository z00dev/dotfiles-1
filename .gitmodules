[submodule ".gitignore-boilerplates"]
	path = .gitignore-boilerplates
	url = git@github.com:simonwhitaker/gitignore-boilerplates.git
[submodule ".oh-my-zsh"]
	path = .oh-my-zsh
	url = git@github.com:robbyrussell/oh-my-zsh.git
[submodule "gitignore-boilerplates-repo"]
	path = gitignore-boilerplates-repo
	url = git@github.com:simonwhitaker/gitignore-boilerplates.git
[submodule "dircolors-solarized"]
	path = dircolors-solarized
	url = git@github.com:seebi/dircolors-solarized.git
[submodule "zsh-syntax-highlighting"]
	path = zsh-syntax-highlighting
	url = git@github.com:zsh-users/zsh-syntax-highlighting.git
[submodule "irssi-colors-solarized"]
	path = irssi-colors-solarized
	url = git@github.com:huyz/irssi-colors-solarized.git
	ignore = dirty
[submodule ".tmux/plugins/tmp"]
	path = .tmux/plugins/tmp
	url = https://github.com/tmux-plugins/tpm
[submodule ".tmux/plugins/tpm"]
	path = .tmux/plugins/tpm
	url = https://github.com/tmux-plugins/tpm
