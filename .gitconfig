[user]
	name = Xandor Schiefer
	email = zeorin@gmail.com
[core]
	editor = vim
	excludesfile = /home/ubuntu/.gitignore
[merge]
	tool = vimdiff
[credential]
	helper = cache
[color]
	ui = auto
[alias]
	co = checkout
	br = branch
	ci = commit
	st = status
	unstage = reset HEAD --
	last = log -1 HEAD
	visual = !ungit
	stash-unapply = !git stash show -p | git apply -R
